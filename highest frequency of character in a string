#include<stdio.h>
#include<string.h>
#include<stdlib.h>
void main()
{
   char str[100];
   int freq[255],ascii,i,max;
   printf("enter a string:");
   fgets(str,sizeof(str),stdin);
   for(i=0;i<255;i++)
   {
       freq[i]=0;
   }
   for(i=0;str[i]!='\0';i++)
   {
     ascii=(int)str[i];
     freq[ascii]++;
   }
   max=0;
   for(i=0;i<255;i++)
   {
       if(i!=32)
       {
           if(freq[i]>freq[max])
           {
               max=i;
           }
       }
   }
   printf("%c has the high frequency %d!",max,freq[max]);
   
}
