#include <stdio.h>

void main()
{
   int i,j,k,l;
   for(i=0;i<5;i++)
   {
       for(j=0;j<5-i;j++)
       {
           printf("*");
       }
       for(j=0;j<2*i;j++)
       {
           printf(" ");
       }
       for(j=0;j<5-i;j++)
       {
           printf("*");
       }
       
      printf("\n");
   }
   for(k=1;k<=5;k++)
   {
       for(l=1;l<=k;l++)
       {
           printf("*");
       }
       for(l=0;l<=2*(5-k)-1;l++)
       {
           printf(" ");
       }
       for(l=1;l<=k;l++)
       {
           printf("*");
       }
       
       printf("\n");
   }
}
