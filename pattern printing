#include <stdio.h>

void main() 
{
   int i,j,n,k=1;
   printf("enter number of rows:");
   scanf("%d",&n);
   for(i=1;i<=n;i++)
   {
       for(j=1;j<=n-i;j++)
           {
               printf("\t");
           }
       for(j=1;j<=i;j++)
           {
               printf("%d \t \t",k++);
           }
      printf("\n");
    }
}
